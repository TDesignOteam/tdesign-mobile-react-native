{
  "name": "site",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "move": "node ./scripts/move-example.js",
    "start": "npm run move && vite",
    "start:skip": "npm run move -- --skip-move && vite",
    "dev": "npm run move && vite",
    "dev:skip": "npm run move -- --skip-move && vite",
    "build": "npm run move && vite build",
    "serve": "npm run move && vite preview",
