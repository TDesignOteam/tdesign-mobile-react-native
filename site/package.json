    "preview": "npm run move && vite build --mode preview",
    "postsite:preview": "cp _site/index.html _site/404.html"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.20.13",
    "@originjs/vite-plugin-commonjs": "^1.0.3",
