{
  "name": "tdesign-react-native",
  "version": "0.0.1",
  "description": "TDesign Component for React Native",
  "main": "index.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "prepare": "husky install",
    "eslint": "eslint --max-warnings 0 --ext js,jsx,ts,tsx --cache",
    "check": "cross-env NODE_ENV=production npm run eslint",
    "test": "jest",
    "postinstall": "cd example && npm i && cd ../site && npm i",
    "site:start": "cd site && npm run start",
    "site:dev": "cd site && npm run dev",
    "site:build": "cd site && npm run build",
    "site:serve": "cd site && npm run preview",
    "site:intranet": "cd site && npm run intranet",
    "site:preview": "cd site && npm run preview",
    "postsite:preview": "cp _site/index.html _site/404.html"
  },
  "author": "tdesign",
  "license": "MIT",
  "dependencies": {
    "hoist-non-react-statics": "^3.3.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.3",
    "react-native-reanimated": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.20.0",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.18.6",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.3",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.6",
    "@types/react": "^18.0.26",
    "@types/react-native": "^0.71.0",
    "@types/react-test-renderer": "^18.0.0",
    "chalk": "^4.1.0",
    "eslint": "^8.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.22.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "prismjs": "^1.29.0",
    "react-router-dom": "^6.8.0"
  },
  "peerDependencies": {
    "eslint-config-react-app": "^7.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
